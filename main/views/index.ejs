<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Loans Table</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }
    th, td {
      text-align: left;
      padding: 8px;
      border-bottom: 1px solid #ddd;
    }
    tr:hover {
      background-color: #f5f5f5;
    }
  </style>
</head>
<body>
  <h1>Loans Table</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Customer Name</th>
        <th>Phone Number</th>
        <th>Address</th>
        <th>Loan Amount</th>
        <th>Interest</th>
        <th>Loan Term Years</th>
        <th>Loan Type</th>
        <th>Description</th>
        <th>Created Date</th>
        <th>Inserted Date</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < data.loans.length; i++) { %>
        <tr>
          <td><%= data.loans[i].id %></td>
          <td><%= data.loans[i].customerName %></td>
          <td><%= data.loans[i].phoneNumber %></td>
          <td><%= data.loans[i].address %></td>
          <td><%= data.loans[i].loanAmount %></td>
          <td><%= data.loans[i].interest %></td>
          <td><%= data.loans[i].loanTermYears %></td>
          <td><%= data.loans[i].loanType %></td>
          <td><%= data.loans[i].description %></td>
          <td><%= data.loans[i].createdDate %></td>
          <td><%= data.loans[i].insertedDate %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>
